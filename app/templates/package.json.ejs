{
  "name": "<%= appName %>",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "postinstall": "if [ $NPM_CONFIG_PRODUCTION ]; then npm run publish; fi",
    "dev": "nodemon src/server/index.js --exec babel-node",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "webpack --progress -p && mv dist/assets/index.html dist && cp -R public/ dist/",
    "build:server": "babel --ignore src/client -d ./dist ./src -s",
    <% if (serverlessInfrastructure) { %>
    "lambda:generate": "npm run build && claudia generate-serverless-express-proxy --express-module dist/server/app.production",
    "lambda:create": "claudia create --handler lambda.handler --deploy-proxy-api --region <%= serverlessRegion %>",
    "lambda:update": "claudia update"
    <% } else { %>
    "start": "pm2 start dist/server -i max",
    "start-docker": "pm2-docker start dist/server -i max"
    <% } %>
  },
  "dependencies": {
    <% if (serverlessInfrastructure) { %>"aws-sdk": "^2.252.1",<% } %>
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    <% if (serverlessInfrastructure) { %>"claudia": "^4.0.0",<% } %>
    "css-loader": "^0.28.9",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
    "file-loader": "^1.1.6",
    "helmet": "^3.10.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.20.0",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "pm2-docker": "^2.1.0",
    "postcss-custom-properties": "^6.2.0",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-loader": "^2.0.10",
    "postcss-utilities": "^0.7.0",
    "prerender-node": "^2.7.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.2.1",
    "redux-form-material-ui": "^4.3.3",
    "redux-multi": "^0.1.12",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "validator": "^9.2.0",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "nodemon": "^1.14.11",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  }
}
